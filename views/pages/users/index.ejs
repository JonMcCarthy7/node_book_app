<div class="card card-default">
  <div class="card-body">
    <div class="row">
      <div class="card-columns mx-auto">
        <%# clubs.forEach( club => { %>
        <div class="col-md-4">
          <a href="/books/<%#=club.book_id %>" class="">
            <div class="card text-center" style="width: 18rem;">
              <img class="card-img-top" src="<%#= book.img_url %>" alt="<%#=book.title%>">
              <div class="card-body">
                <h5 class="card-title">
                  <%#club.name%>
                </h5>
              </div>
            </div>
          </a>
        </div>
        <%# }) %>
      </div>
    </div>
  </div>
</div>